backend rabbitmq-web-backend
  balance source
  option httpchk GET /
  http-check expect status 200
  reqrep ^([^\ :]*)\ /rabbitmq/(.*) \1\ /\2
<% @servers.each do |server| -%>
  <%= "server #{server['hostname']} #{server['bcpc']['management']['ip']}:55672 check inter 5s rise 1 fall 1" %>
<% end -%>
